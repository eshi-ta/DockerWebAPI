@model List<Event>
<table class="table table-striped table-responsive ">
    <thead>
        <th>Title</th>
        <th>Action</th>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td><a asp-controller="Event" asp-action="EventDetails" asp-route-id="@obj.Id">@obj.Title</a></td>
                <td><a class="btn btn-warning" asp-controller="Event" asp-action="Edit" asp-route-id="@obj.Id">Edit</a></td>
            </tr>
        }
    </tbody>
</table>